{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
          "@web-mock/common": [
            "../common/src"
          ],
          "@web-mock/common/*": [
            "../common/src/*"
          ]
        },
        "esModuleInterop": true,
        "skipLibCheck": true,
        "noEmit": true
    },
    "references": [
        {
            "path": "@web-mock/common"
        },
        {
            "path": "@web-mock/frontend"
        },
        {
            "path": "@web-mock/backend"
        },
        {
            "path": "@web-mock/cli"
        },
        {
            "path": "@web-mock/db"
        }
    ]
}